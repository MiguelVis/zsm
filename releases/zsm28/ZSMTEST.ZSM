;
; PSEUDO OPS
;
	ORG	100H
;
	DEFB	0AAH
NNNN:	DEFS	2		;SAMPLE	ADDRESS
	DEFW	0AABBH
L1	EQU	0AABBH
INDEX	EQU	5		;SAMPLE	INDEX
NN	EQU	20H		;SAMPLE	8 BIT VALUE
DIS	EQU	30H
;
	IF	0
; SHOULD NOT BE	ASSEMBLED
	LD	A,B
	ENDIF
	IF	1
; SHOULD BE ASSEMBLED
	LD	A,B
	ENDIF
; TURN LISTING OFF
	LIST	OFF
; LISTING SHOULD NOT BE	ON
	LD	A,B
	LIST	ON
; LISTING SHOULD BE ON
;
;	KNOWN PREVIOUS ERRORS IN Z80ASMUK
;
	LD	(HL),-1
	CP	-1
	LD	DE,-6
	LD	A,+6
	LD	A,-6
